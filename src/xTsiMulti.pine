FunxTsiMu_inRange(_cond) =>
	_range_upper						= 30			//	Rango de correcciÃ³n maximo
	_range_lower						= 2				//	Rango de correcciÃ³n minimo
	_bars_since							= ta.barssince(_cond == true)

	_range_lower <= _bars_since and _bars_since <=  _range_upper


/// Compra regular â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€


